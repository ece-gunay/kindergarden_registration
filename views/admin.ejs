<!DOCTYPE html>
<html>
<head>
  <title>Admin Panel</title>
  <style>
    table, th, td {
      border: 1px solid black;
      border-collapse: collapse;
    }
    th, td {
      padding: 10px;
      text-align: left;
    }
  </style>
</head>
<body>
  <h1>Admin Panel</h1>
  <table>
    <tr>
      <th>Name</th>
      <th>ID No</th>
      <th>Phone</th>
      <!-- Add other headers as needed -->
    </tr>
    <% users.forEach(user => { %>
    <tr>
      <td><%= user.nameSurname %></td>
      <td><%= user.IDNo %></td>
      <td><%= user.phone %></td>
      <!-- Add other user data as needed -->
    </tr>
    <% }); %>
  </table>
</body>
</html>
